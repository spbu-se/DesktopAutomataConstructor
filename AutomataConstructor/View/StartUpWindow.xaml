<Window x:Class="AutomataConstructor.View.StartUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutomataConstructor.View"
        mc:Ignorable="d"
        Title="StartUpWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <ComboBox SelectionChanged="ComboBox_SelectionChanged" x:Name="langageComboBox">
                <ComboBoxItem Content="English"/>
                <ComboBoxItem Content="Русский"/>
            </ComboBox>
            <Button Content="Start" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
